{
  "id": "revolutionary-pbshipgen",
  "title": "Pirate Borg Ship Generator",
  "description": "A module for Foundry VTT and Pirate Borg for generating random ships and their captains.",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "Jason Godesky",
      "email": "jason@revolutionarygames.co",
      "github": "revolutionarygamesco"
    }
  ],
  "esmodules": [
    "scripts/module.js"
  ],
  "styles": [
    "styles/styles.css"
  ],
  "packs": [
    {
      "name": "macros",
      "label": "Pirate Borg Ship Generator Macros",
      "path": "packs/macros",
      "type": "Macro",
      "ownership": {
        "GAMEMASTER": "OWNER"
      }
    },
    {
      "name": "rolltables",
      "label": "Pirate Borg Ship Generator Roll Tables",
      "path": "packs/rolltables",
      "type": "RollTable",
      "ownership": {
        "GAMEMASTER": "OWNER"
      }
    },
    {
      "name": "actors",
      "label": "Pirate Borg Ship Generator Actors",
      "path": "packs/actors",
      "type": "Actor",
      "ownership": {
        "GAMEMASTER": "OWNER"
      }
    },
    {
      "name": "features",
      "label": "Pirate Borg Ship Generator Specialty Crew Features",
      "path": "packs/features",
      "type": "Item",
      "ownership": {
        "GAMEMASTER": "OWNER"
      }
    }
  ],
  "packFolders": [
    {
      "name": "Pirate Borg Ship Generator",
      "sorting": "a",
      "packs": [
        "rolltables",
        "macros",
        "features",
        "actors"
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "pirateborg",
        "type": "system",
        "compatibility": {}
      }
    ],
    "requires": [
      {
        "id": "revolutionary-piratenames",
        "type": "module",
        "compatibility": {}
      }
    ]
  },
  "socket": false,
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "bugs": "#{BUGS}#",
  "changelog": "#{CHANGELOG}#",
  "license": "#{LICENSE}#",
  "readme": "#{README}#"
}
